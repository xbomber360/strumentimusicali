<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/Home.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>navigazione</from-outcome>
            <to-view-id>/prova.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>navigaRegistrazione</from-outcome>
            <to-view-id>/Registrazione.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>navigaLogin</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>navigaCarrello</from-outcome>
            <to-view-id>/Carrello.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ritornoHome</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Carrello.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>navigaLogin</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>dataMenuBean</managed-bean-name>
        <managed-bean-class>managedBean.DataMenuBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>gestioneProdottoBean</managed-bean-name>
        <managed-bean-class>managedBean.GestioneProdottoBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>managedBean.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>registrazione</managed-bean-name>
        <managed-bean-class>managedBean.Registrazione</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>provinciaManaged</managed-bean-name>
        <managed-bean-class>managedBean.ProvinciaManaged</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>carrelloManagerBean</managed-bean-name>
        <managed-bean-class>managedBean.CarrelloManagerBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
</faces-config>
